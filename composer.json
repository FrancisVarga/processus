{
    "name":"processus/unicorn",
    "type":"framework",
    "keywords":["jsonrpc", "games", "rpc", "framework"],
    "homepage":"http://github.com/Crowdpark/processus",
    "description":"JSON-RPC Framework for Apps and Games",
    "license":"MIT",
    "authors":[
        {
            "name":"Francis Varga",
            "email":"fv@crowdpark.com",
            "homepage":"http://crowdpark.com"
        }
    ],
    "support":{
        "issues":"https://github.com/Crowdpark/processus/issues"
    },
    "autoload":{
        "psr-0":{
            "Processus\\":"core/"
        }
    },
    "repositories":[
        {
            "type":"composer",
            "url":"http://packages.zendframework.com/"
        },
        {
            "type":"vcs",
            "url":"https://github.com/Crowdpark/processus-schnarchnase.git"
        },
        {
            "type":"vcs",
            "url":"https://github.com/bastman/processus-jsonrpc-pipe.git"
        },
        {
            "type":"vcs",
            "url":"https://github.com/Crowdpark/processus-spl.git"
        },

        {
            "type":"vcs",
            "url":"https://github.com/Crowdpark/processus-ruhebett.git"
        },
        {
            "type":"vcs",
            "url":"https://github.com/Crowdpark/php-ga.git"
        },
        {
            "type":"vcs",
            "url":"https://github.com/FrancisVarga/monolog.git"
        }
    ],
    "require":{
        "php":">=5.3",
        "zendframework/zend-db":"2.0",
        "zendframework/zend-server":"2.0",
        "zendframework/zend-http":"2.0",
        "zendframework/zend-json":"2.0",
        "processus/ruhebett":"dev-master",
        "processus/schnarchnase":"dev-master",
        "processus/jsonrpcpipe":"dev-master",
        "processus/spl":"dev-master",
        "mustache/mustache":"*",
        "swiftmailer/swiftmailer":"master",
        "pda/pheanstalk":"dev-master",
        "riak/riak-client":"*",
        "facebook/php-sdk":"dev-master",
        "unitedprototype/php-ga":"dev-master",
        "monolog/monolog":"master",
        "symfony/console":"dev-master"
    },
    "bin":[
        "bin/speedy.php",
        "bin/worker.sh",
        "console/cli.php"
    ],
    "minimum-stability":"dev"
}